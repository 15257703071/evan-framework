<ehcache>
    <diskStore path="java.io.tmpdir"/>

    <!-- 数据字典 列表-->
    <cache name="DataDictionaryCacheForData"
           maxElementsInMemory="1024"
           timeToIdleSeconds="90"
           timeToLiveSeconds="90">
    </cache>
    <!-- 数据字典 code 名称-->
    <cache name="DataDictionaryCacheForName"
           maxElementsInMemory="2048"
           timeToIdleSeconds="90"
           timeToLiveSeconds="90">
    </cache>


</ehcache>
