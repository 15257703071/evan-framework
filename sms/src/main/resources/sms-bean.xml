<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="smsService" class="com.ancun.core.sms.impl.SMSServiceImpl" init-method="init">
		<property name="smsServerUrl" value="${sms.service.url}"></property>
		<property name="smsServerUser" value="${sms.service.user}"></property>
		<property name="smsServerPwd" value="${sms.service.pwd}"></property>
	</bean>
</beans>